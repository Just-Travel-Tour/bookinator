        "EC2"
    ],
    "cpu": "512",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": [
        {
            "key": "Environment",
            "value": "prod"
        },
        {
            "key": "ManagedBy",
            "value": "Manual"
        },
        {
            "key": "Name",
            "value": "bookinator-prod"
        }
    ]
}